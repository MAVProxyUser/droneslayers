<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/i0" />
            <solid android:color="@color/n7" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/i0" />
            <stroke android:width="@dimen/fz" android:color="@color/om" />
        </shape>
    </item>
</selector>